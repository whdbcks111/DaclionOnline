# 환경 변수 예제 파일
# 이 파일을 복사해서 .env 파일을 만드세요

# 서버 포트
PORT=6602

# CORS 설정 (개발 환경)
CORS_ORIGIN=http://localhost:5173

# MariaDB 데이터베이스 설정
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=daclion_db

# Prisma DATABASE_URL
DATABASE_URL="mysql://root:root@localhost:3306/daclion_db"

# Gmail SMTP 설정
MAIL_USER=your_email@gmail.com
MAIL_APP_PASSWORD=your_app_password